<%= form_for @attraction do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name, value: @attraction.name %>
  <%= f.label :nausea_rating %>
  <%= f.text_field :nausea_rating, value: @attraction.nausea_rating %>
  <%= f.label :happiness_rating %>
  <%= f.text_field :happiness_rating, value: @attraction.happiness_rating %>
  <%= f.label :tickets %>
  <%= f.text_field :tickets, value: @attraction.tickets %>
  <%= f.label :min_height %>
  <%= f.text_field :min_height, value: @attraction.min_height %>
  
  <%= f.submit "Update Attraction" %>
<% end %>
